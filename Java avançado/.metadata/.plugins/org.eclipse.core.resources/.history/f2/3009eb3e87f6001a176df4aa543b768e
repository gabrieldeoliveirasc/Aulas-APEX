		MarcaModelo obj4 = new Marcamodelo("Samsung", 4);
		MarcaModelo obj5 = new Marcamodelo("Sony", 5);
		
		//DefaultComboBoxModel
		DefaultComboBoxModel<E>oxModel
odelo> lst = new DefaultComboBoxModel<MarcaModelo>();
